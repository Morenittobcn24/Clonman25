{
  "kind": "collectionType",
  "collectionName": "actividades",
  "info": {
    "singularName": "actividad",
    "pluralName": "actividades",
    "displayName": "Actividad"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Nombre": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Descripcion": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Imagen": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Precio": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Duracion": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "viajes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::viaje.viaje",
      "inversedBy": "Actividades"
    },
    "reservas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::reserva.reserva",
      "inversedBy": "Actividades"
    }
  }
}
