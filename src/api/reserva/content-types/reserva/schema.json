{
  "kind": "collectionType",
  "collectionName": "reservas",
  "info": {
    "singularName": "reserva",
    "pluralName": "reservas",
    "displayName": "Reserva",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Localizador": {
      "type": "uid"
    },
    "Fecha_reserva": {
      "type": "date",
      "required": true
    },
    "cliente": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cliente.cliente"
    },
    "Personas": {
      "type": "integer"
    },
    "Estado": {
      "type": "enumeration",
      "enum": [
        "Pendiente",
        "Confirmada",
        "Cancelada"
      ]
    },
    "Precio_total": {
      "type": "decimal"
    },
    "Forma_pago": {
      "type": "enumeration",
      "enum": [
        "Online",
        "Transferencia",
        "Manual"
      ]
    },
    "Reserva_pagada": {
      "type": "boolean",
      "default": false
    },
    "Canal": {
      "type": "enumeration",
      "enum": [
        "Web",
        "Interno",
        "Agencia",
        "Whatsapp"
      ]
    },
    "Notas_internas": {
      "type": "string"
    },
    "Viaje": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::viaje.viaje"
    },
    "Integrantes": {
      "type": "component",
      "component": "reservas.integrante-grupo",
      "repeatable": true
    },
    "Salida": {
      "type": "component",
      "component": "viajes.salida",
      "repeatable": false
    },
    "Alojamiento": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::alojamiento.alojamiento",
      "mappedBy": "reserva"
    },
    "Actividades": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::actividad.actividad",
      "mappedBy": "reservas"
    },
    "Seguro": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::seguro.seguro",
      "inversedBy": "reservas"
    }
  }
}
